  link String?

}

model users{
  id Int @id @default(autoincrement())
  username String @db.VarChar(255) @unique
  password String @db.VarChar(255)
  email String @db.VarChar(255) @unique
  createdAt DateTime @default(now())
  updated DateTime @updatedAt

}